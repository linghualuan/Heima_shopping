.content_swiper swiper {
  height: calc(100vw * 340 / 750);
}
.content_swiper swiper navigator image {
  width: 100%;
}
.cate {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cate .cate_item {
  flex: 1;
}
.cate .cate_item image {
  width: 100%;
}
.floor .floor_item .floor_title image {
  width: 100%;
}
.floor .floor_item .floor_list {
  overflow: hidden;
}
.floor .floor_item .floor_list navigator {
  float: left;
  width: 33.33%;
}
.floor .floor_item .floor_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor .floor_item .floor_list navigator:nth-child(2) {
  border-bottom: 10rpx solid #fff;
}
.floor .floor_item .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor .floor_item .floor_list navigator image {
  width: 100%;
  height: 100%;
}
